{
    "name": "markus-michalski/osticket-tab-swap",
    "description": "osTicket plugin that swaps Reply and Internal Note tabs",
    "type": "osticket-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Markus Michalski",
            "email": "info@markus-michalski.net"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "TabSwap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TabSwap\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:watch": "phpunit --watch"
    }
}
